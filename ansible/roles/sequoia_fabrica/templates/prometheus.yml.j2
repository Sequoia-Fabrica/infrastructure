global:
  scrape_interval: 1m
scrape_configs:
  - job_name: node_exporter
    static_configs:
      - targets: ['192.168.88.6:9100']
  - job_name: frigate_exporter
    static_configs:
      - targets: ["192.168.88.6:9101"]
  - job_name: asterisk
    static_configs:
      - targets: ["192.168.88.6:8888"]
  - job_name: octoprint_prusa_2
    scrape_interval: 5s
    static_configs:
      - targets: ["192.168.88.184"]
        labels:
            octoprint_instance: "prusa-2"
    params:
        apikey:
            - "{{ octoprint_api_keys['prusa_2'] }}"
    metrics_path: '/plugin/prometheus_exporter/metrics'
